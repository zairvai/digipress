{
  "function": {
    "DigipressAuthPreTokenGeneration": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    },
    "DigipressGA": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": [
        {
          "category": "function",
          "resourceName": "baktipesantrenDigipressLayer",
          "attributes": [
            "Arn"
          ]
        }
      ]
    },
    "baktipesantrenDigipressLayer": {
      "providerPlugin": "awscloudformation",
      "service": "LambdaLayer",
      "build": true
    },
    "DigipressIdentityManager": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": [
        {
          "category": "auth",
          "resourceName": "DigipressAuth",
          "attributes": [
            "UserPoolId"
          ]
        }
      ]
    },
    "DigipressStorageManager": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": [
        {
          "category": "storage",
          "resourceName": "DigipressStorage",
          "attributes": [
            "BucketName"
          ]
        }
      ]
    },
    "DigipressImageResize": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": [
        {
          "category": "function",
          "resourceName": "baktipesantrenDigipressLayer",
          "attributes": [
            "Arn"
          ]
        }
      ]
    },
    "DigipressSearchManager": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": []
    },
    "DigipressTableAppTrigger": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": [
        {
          "category": "function",
          "resourceName": "DigipressIdentityManager",
          "attributes": [
            "Name"
          ]
        },
        {
          "category": "function",
          "resourceName": "DigipressStorageManager",
          "attributes": [
            "Name"
          ]
        },
        {
          "category": "function",
          "resourceName": "DigipressSearchManager",
          "attributes": [
            "Name"
          ]
        }
      ]
    }
  },
  "auth": {
    "DigipressAuth": {
      "service": "Cognito",
      "providerPlugin": "awscloudformation",
      "dependsOn": [
        {
          "category": "function",
          "resourceName": "DigipressAuthPreTokenGeneration",
          "triggerProvider": "Cognito",
          "attributes": [
            "Arn",
            "Name"
          ]
        }
      ],
      "customAuth": false
    }
  },
  "api": {
    "DigipressAPI": {
      "service": "AppSync",
      "providerPlugin": "awscloudformation",
      "output": {
        "authConfig": {
          "defaultAuthentication": {
            "authenticationType": "AMAZON_COGNITO_USER_POOLS",
            "userPoolConfig": {
              "userPoolId": "authDigipressAuth"
            }
          },
          "additionalAuthenticationProviders": []
        }
      }
    }
  },
  "storage": {
    "DigipressStorage": {
      "service": "S3",
      "providerPlugin": "awscloudformation"
    }
  }
}